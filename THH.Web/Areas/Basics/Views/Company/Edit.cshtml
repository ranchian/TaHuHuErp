@model THH.Model.ReportModel.Dto.CompanyDto
@{
    ViewBag.Title = "Edit";
}

<script type="text/javascript">
    $(document).ready(function () {
        $("form").validate();
    });
</script>
@using (Html.BeginForm())
{
<div style=" margin:15px; border:1px">
    <div class="form-group">
        <label for="name">公司名称</label>
        @*<input type="text" class="form-control" id="UserName" placeholder="公司名称" required>*@
        @Html.TextBoxFor(m => m.Name, new { @class = "form-control", placeholder = "公司名称", required = "required" })
    </div>
    <div class="form-group">
        <label for="name">公司编号</label>
        @*<input type="text" class="form-control" id="LoginName" placeholder="公司编号" required>*@
        @Html.TextBoxFor(m => m.Code, new { @class = "form-control", placeholder = "公司编号", required = "required" })
    </div>
    <div class="form-group">
        <label for="name">地址</label>
        @*<input type="text" class="form-control" id="Email" placeholder="地址" required>*@
        @Html.TextBoxFor(m => m.Address, new { @class = "form-control", placeholder = "地址", required = "required" })
    </div>
    <div class="form-group">
        <label for="name">創建人</label>
        @Html.TextBoxFor(m => m.CredateTime, new { @class = "form-control",  required = "required" })
    </div>
    <div class="form-group">
        <label for="name">創建時間</label>
        @Html.TextBoxFor(m => m.CreateUser, new { @class = "form-control",  required = "required" })
    </div>
    <div>
        <div style="margin-left:70%">
            <button type="submit" class="btn btn-primary">提交</button>
        </div>
    </div>
</div>
}


